<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="jmu.fly.mapper.机场Mapper">

	<select id="find机场By机场名称" parameterType="String" resultType="jmu.fly.po.机场">
		select * from 机场 where 机场名称 like #{机场名称}
	</select>

</mapper>